// User LED blinky API

Interface(
    name: "UserLeds",
    ops: {
        "led_on": (
            args: {
                "index": "usize",
            },
            reply: Result(
                ok: "()",
                err: CLike("LedError"),
            ),
            idempotent: true,
        ),
        "led_off": (
            args: {
                "index": "usize",
            },
            reply: Result(
                ok: "()",
                err: CLike("LedError"),
            ),
            idempotent: true,
        ),
        "led_toggle": (
            args: {
                "index": "usize",
            },
            reply: Result(
                ok: "()",
                err: CLike("LedError"),
            ),
            idempotent: true,
        ),
        "led_set_state": (
            args: {
                "index": "usize",
                "state": {
                  type: "LedState",
                  recv: FromPrimitive("u8"),
                }
            },
            reply: Result(
                ok: "()",
                err: CLike("LedError"),
            ),
            idempotent: true,
        ),
    },
)
